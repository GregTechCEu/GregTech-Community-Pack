<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config>
    <lines at="topleft">
        <line>
            <str>{pingicon}$6{username}$f </str>
            <str>FPS: </str>
            <op>
                <str>gt</str>
                <var>fps</var>
                <num>60</num>
                <num>45</num>
                <num>30</num>
                <num>15</num>
                <num>0</num>
                <str>$a</str>
                <str>$e</str>
                <str>$6</str>
                <str>$c</str>
                <str>$4</str>
                <str/>
            </op>
            <var>fps</var>
        </line>
        <line>
            <icon>
                <str>minecraft:clock</str>
            </icon>
            <str> $rMinecraft Year $3</str>
            <add>
                <round>
                    <div>
                        <sub>
                            <var>day</var>
                            <modi>
                                <var>day</var>
                                <num>365</num>
                            </modi>
                        </sub>
                        <num>365</num>
                    </div>
                    <num>0</num>
                </round>
                <num>1</num>
            </add>
            <str> $rday $3</str>
            <modi>
                <var>day</var>
                <num>365</num>
            </modi>
            <str>$f, Time: </str>
            <str>$6{mctime12} $r</str>
            <if>
                <var>daytime</var>
                <str>$e(day)</str>
                <str>$7(night)</str>
            </if>
        </line>
        <line>
            <icon>
                <str>HardcoreEnderExpansion:temple_end_portal</str>
            </icon>
            <str> $rDimension: $6{dimension} ({dimensionid}) </str>
            <str> $rChunks Loaded: $6{loadedchunks} </str>
        </line>
        <line>
            <icon>
                <str>minecraft:sapling</str>
                <num>4</num>
            </icon>
            <str> Biome: $3{biome}$f</str>
            <str> $6{temperature}$r°F $6</str>
            <round>
                <div>
                    <sub>
                        <var>temperature</var>
                        <num>32</num>
                    </sub>
                    <num>1.8</num>
                </div>
                <num>0</num>
            </round>
            <str>$r°C $6</str>
            <round>
                <add>
                    <div>
                        <sub>
                            <var>temperature</var>
                            <num>32</num>
                        </sub>
                        <num>1.8</num>
                    </div>
                    <num>273.15</num>
                </add>
                <num>0</num>
            </round>
            <str>$rK</str>
            <str>  $6{humidity}%$f humidity</str>
        </line>
        <line>
            <icon>
                <str>minecraft:skull</str>
                <num>4</num>
            </icon>
            <str> Entities: $6{entitiesrendered}$f rendered / $6{entitiestotal}$f loaded  </str>
        </line>
        <line>
            <icon>
                <str>minecraft:compass</str>
                <num>0</num>
            </icon>
            <str> Chunk X:$6 {chunkx} $fZ:$6 {chunkz}$f Off: $3$6 {chunkoffsetx} $f:$6 {chunkoffsetz} $f</str>
            <str> Y: $6{yfeeti}$f facing $6{finedirection}$f</str>
            <if>
                <var>slimechunk</var>
                <str> $6SlimeChunk$f</str>
            </if>
        </line>
        <line>
            <icon>
                <str>minecraft:torch</str>
            </icon>
            <str> Light: </str>
            <max>
                <var>light</var>
                <num>7.5</num>
                <str>$e</str>
                <str>$c</str>
            </max>
            <var>light</var>
            <str>$r (feet: </str>
            <max>
                <var>lightfeet</var>
                <num>7.5</num>
                <str>$e</str>
                <str>$c</str>
            </max>
            <var>lightfeet</var>
            <str>$r)</str>
        </line>
        <line>
            <str>$0</str>
        </line>
        <line>
            <if>
                <var>snowing</var>
                <str>  It is $2{underline}Snowing$r$2$r</str>
            </if>
            <if>
                <var>raining</var>
                <str>   It is $2{underline}Raining$r$2$r</str>
            </if>
            <if>
                <var>thundering</var>
                <str> and $2{u}Thunderin$r$2g$f.</str>
            </if>
        </line>
        <line>
            <str>$0</str>
        </line>
        <line>
            <if>
                <and>
                    <or>
                        <equal>
                            <modi>
                                <var>chunkx</var>
                                <num>3</num>
                            </modi>
                            <num>-1</num>
                        </equal>
                        <equal>
                            <modi>
                                <var>chunkx</var>
                                <num>3</num>
                            </modi>
                            <num>1</num>
                        </equal>
                    </or>
                    <or>
                        <equal>
                            <modi>
                                <var>chunkz</var>
                                <num>3</num>
                            </modi>
                            <num>-1</num>
                        </equal>
                        <equal>
                            <modi>
                                <var>chunkz</var>
                                <num>3</num>
                            </modi>
                            <num>1</num>
                        </equal>
                    </or>
                </and>
                <concat>
                    <icon>
                        <str>textures/items/diamond_pickaxe.png</str>
                        <num>0</num>
                        <num>0</num>
                        <num>16</num>
                        <num>16</num>
                    </icon>
                    <str>  This is an $2{underline}ore$f chunk.</str>
                </concat>
            </if>
        </line>
        <line>
            <if>
                <and>
                    <equal>
                        <var>chunkoffsetx</var>
                        <num>7</num>
                    </equal>
                    <equal>
                        <var>chunkoffsetz</var>
                        <num>7</num>
                    </equal>
                </and>
                <concat>
                    <if>
                        <and>
                            <or>
                                <equal>
                                    <modi>
                                        <var>chunkx</var>
                                        <num>3</num>
                                    </modi>
                                    <num>-1</num>
                                </equal>
                                <equal>
                                    <modi>
                                        <var>chunkx</var>
                                        <num>3</num>
                                    </modi>
                                    <num>1</num>
                                </equal>
                            </or>
                            <or>
                                <equal>
                                    <modi>
                                        <var>chunkz</var>
                                        <num>3</num>
                                    </modi>
                                    <num>-1</num>
                                </equal>
                                <equal>
                                    <modi>
                                        <var>chunkz</var>
                                        <num>3</num>
                                    </modi>
                                    <num>1</num>
                                </equal>
                            </or>
                        </and>
                        <str>      </str>
                    </if>
                    <str>  You are at the $4center$f of the chunk.</str>
                </concat>
            </if>
        </line>
        <line>
            <str>$0</str>
        </line>
        <line>
            <if>
                <str>  Nearest players:</str>
            </if>
        </line>
        <line>
            <if>
                <str>  1) $6{nearbyplayername0}$f({nearbyplayerdistance0}m)</str>
            </if>
        </line>
        <line>
            <if>
